{
  "description": "__MSG_extensionDescription__",
  "manifest_version": 3,
  "name": "CheckMyHTTPS",
  "version": "5.7.1",
  "default_locale": "en",
  "icons": {
    "128": "icons/icon.png"
  },
  "author": "CheckMyHttps Team",
  "homepage_url": "https://checkmyhttps.net",
  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "79.0"
    }
  },
  "background": {
    "page": "background.html"
  },
  "action": {
    "default_icon": {
      "128": "icons/icon.png"
    }
  },
  "options_ui": {
    "browser_style": false,
    "open_in_tab": false,
    "page": "options/options.html"
  },
  "permissions": [
    "notifications",
    "storage",
    "tabs",
    "webRequest",
    "webRequestBlocking"
  ],
  "host_permissions":[
    "<all_urls>"
  ]
}
