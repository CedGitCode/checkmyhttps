{
  "description": "__MSG_extensionDescription__",
  "manifest_version": 2,
  "name": "CheckMyHTTPS",
  "version": "5.1.0",
  "default_locale": "en",
  "icons": {
    "64": "images/icon.png"
  },
  "applications": {
    "gecko": {
      "id": "info@checkmyhttps.net",
      "strict_min_version": "50.0"
    }
  },
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "browser_style": false,
    "default_icon": {
      "64": "images/icon.png"
    }
  },
  "options_ui": {
    "browser_style": true,
    "open_in_tab": false,
    "page": "options/options.html"
  },
  "permissions": [
    "downloads",
    "nativeMessaging",
    "notifications",
    "storage",
    "tabs",
    "webRequest",
    "https://*/*"
  ]
}
